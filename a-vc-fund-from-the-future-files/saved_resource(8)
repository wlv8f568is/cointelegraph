/* Cointraffic.io */

(function () {
var ct_load = function(){
var ct_script = document.createElement('script');
ct_script.type = "text/javascript";
ct_script.src = "//apps2.cointraffic.io/bnr/?wkey=7Lhbqh";
document.getElementsByTagName('head')[0].appendChild(ct_script);

var ct2_script = document.createElement('script');
ct2_script.type = "text/javascript";
ct2_script.src = "//ads.creative-serving.com/pixel?id=3143681&type=js";
document.getElementsByTagName('head')[0].appendChild(ct2_script);
};
if (window.addEventListener) {
window.addEventListener('load', ct_load, false);
} else {
window.attachEvent('onload', ct_load)
}
}());
        